<tr>
  <td class="text-center py-auto">#</td>
  <td><%= _test.title %></td>
  <td><%= subject %></td>
  <td><%= _test.description %></td>
  <td><%= _test.start_time %></td>
  <td><%= _test.due_time %></td>
  <td><%= _test.doing_time %></td>
  <td><%= _test.chapter_name %></td>
  <td><%= _test.mark_name %></td>
  <td><%= l _test.created_at, format: :default %></td>

  <td class="td-actions text-right">
    <button type="button" rel="tooltip" class="btn btn-fab btn-round btn-sm btn-success">
      <i class="material-icons">share</i>
    </button>
    <button type="button" rel="tooltip" class="btn btn-fab btn-round btn-sm btn-info">
      <i class="material-icons">insert_link</i>
    </button>
  </td>
</tr>
